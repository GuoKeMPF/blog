{
  "private": true,
  "scripts": {
    "dev": "cross-env UMI_ENV=dev max dev",
    "start": "cross-env UMI_ENV=start max dev",
    "build": "max build",
    "postinstall": "max generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@umijs/max": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mockjs": "^1.0.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
